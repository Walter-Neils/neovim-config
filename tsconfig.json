{
  "$schema": "https://raw.githubusercontent.com/TypeScriptToLua/TypeScriptToLua/master/tsconfig-schema.json",
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext"],
    "moduleResolution": "Node",
    "types": [
      "lua-types/jit"
    ],
    "jsx": "preserve",
    "jsxFactory": "NUIcreateElement",
    "jsxFragmentFactory": "NUIcreateFragment",
    "strict": true
  },
  "tstl": {
    "noHeader": true,
    "luaTarget": "JIT",
    "luaBundle": "init.lua",
    "luaBundleEntry": "main.ts",
    "noImplicitSelf": true
  }
}
